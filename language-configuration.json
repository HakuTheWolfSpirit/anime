{
	"comments": {
		"lineComment": "#",
		"blockComment": ["/*", "*/"]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		{ "open": "{", "close": "}" },
		{ "open": "[", "close": "]" },
		{ "open": "(", "close": ")" },
		{ "open": "\"", "close": "\"", "notIn": ["string"] }
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""]
	],
	"folding": {
		"markers": {
			"start": "^\\s*\\{",
			"end": "^\\s*\\}"
		}
	},
	"wordPattern": "[a-zA-Z][a-zA-Z0-9_-]*",
	"indentationRules": {
		"increaseIndentPattern": "^.*\\{[^}]*$",
		"decreaseIndentPattern": "^\\s*\\}"
	}
}
